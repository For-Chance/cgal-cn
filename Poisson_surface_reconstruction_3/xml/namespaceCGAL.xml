      <memberdef kind="function" id="group__PkgPoissonSurfaceReconstruction3Ref_1ga2470854cf03552ecee02f11c17024ddf" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
        <templateparamlist>
          <param>
            <type>typename PointInputIterator</type>
          </param>
          <param>
            <type>typename PointMap</type>
          </param>
          <param>
            <type>typename NormalMap</type>
          </param>
          <param>
            <type>typename PolygonMesh</type>
          </param>
          <param>
