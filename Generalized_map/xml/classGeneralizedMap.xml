        <definition>void GeneralizedMap::sew</definition>
        <argsstring>(Dart_descriptor d1, Dart_descriptor d2)</argsstring>
        <name>sew</name>
        <qualifiedname>GeneralizedMap::sew</qualifiedname>
        <param>
          <type>Dart_descriptor</type>
          <declname>d1</declname>
        </param>
        <param>
          <type>Dart_descriptor</type>
