        <name>operator()</name>
        <qualifiedname>CGAL::Barycentric_coordinates::Discrete_harmonic_coordinates_2::operator()</qualifiedname>
        <param>
          <type>const <ref refid="classCGAL_1_1Barycentric__coordinates_1_1Discrete__harmonic__coordinates__2_1a0c78ca3d8dfa681e9cb6f10bd5d22bd2" kindref="member">Point_2</ref> &amp;</type>
