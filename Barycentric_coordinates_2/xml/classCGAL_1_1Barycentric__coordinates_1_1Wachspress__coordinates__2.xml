        <name>weights</name>
        <qualifiedname>CGAL::Barycentric_coordinates::Wachspress_coordinates_2::weights</qualifiedname>
        <param>
          <type>const <ref refid="classCGAL_1_1Barycentric__coordinates_1_1Wachspress__coordinates__2_1a05fb45a3d1ba5f360549ad68435839d1" kindref="member">Point_2</ref> &amp;</type>
