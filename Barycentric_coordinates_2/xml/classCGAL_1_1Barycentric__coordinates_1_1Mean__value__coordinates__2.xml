        <name>operator()</name>
        <qualifiedname>CGAL::Barycentric_coordinates::Mean_value_coordinates_2::operator()</qualifiedname>
        <param>
          <type>const <ref refid="classCGAL_1_1Barycentric__coordinates_1_1Mean__value__coordinates__2_1a1b44432f4e85bd7096ebd7cfa90b3515" kindref="member">Point_2</ref> &amp;</type>
