        <name>is_one_to_one_mapping</name>
        <qualifiedname>CGAL::Surface_mesh_parameterization::Barycentric_mapping_parameterizer_3::is_one_to_one_mapping</qualifiedname>
        <param>
          <type>const <ref refid="classCGAL_1_1Surface__mesh__parameterization_1_1Barycentric__mapping__parameterizer__3_1af6863760cea566c83754304c9d246bdd" kindref="member">Triangle_mesh</ref> &amp;</type>
