        <name>estimate_geodesic_distances</name>
        <qualifiedname>CGAL::Heat_method_3::Surface_mesh_geodesic_distances_3::estimate_geodesic_distances</qualifiedname>
        <param>
          <type>VertexDistanceMap</type>
          <declname>vdm</declname>
