        <param>
          <type>const <ref refid="classCGAL_1_1Arr__geodesic__arc__on__sphere__traits__2_1_1Construct__curve__2_1a48e24310c02414221e7c3fa7b879ec46" kindref="member">Point_2</ref> &amp;</type>
