      <sectiondef kind="user-defined">
      <memberdef kind="function" id="group__PkgArrangementOnSurface2Insert_1gaf7d5aac2640c2a706f8c5e6adc8a7e94" prot="public" static="no" const="no" explicit="no" inline="no" virt="non-virtual">
