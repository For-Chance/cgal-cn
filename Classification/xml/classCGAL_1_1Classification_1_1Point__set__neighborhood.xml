        <name>Point_set_neighborhood</name>
        <qualifiedname>CGAL::Classification::Point_set_neighborhood::Point_set_neighborhood</qualifiedname>
        <param>
          <type>const PointRange &amp;</type>
          <declname>input</declname>
        </param>
